<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>WardCare</servlet-name>
		<servlet-class>org.lds.wardcare.WardCareServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Member</servlet-name>
		<servlet-class>org.lds.wardcare.MemberServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Account</servlet-name>
		<servlet-class>org.lds.wardcare.AccountServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Attendance</servlet-name>
		<servlet-class>org.lds.wardcare.AttendanceServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Attendance</servlet-name>
		<url-pattern>/attendance</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Account</servlet-name>
		<url-pattern>/account</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Member</servlet-name>
		<url-pattern>/member</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WardCare</servlet-name>
		<url-pattern>/wardcare</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
